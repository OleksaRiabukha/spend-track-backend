{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "user": {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "type": {
              "type": "string"
            },
            "attributes": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "integer"
                },
                "first_name": {
                  "type": "string"
                },
                "last_name": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                },
                "spendings": {
                  "type": "array",
                  "items": {}
                }
              },
              "required": [
                "id",
                "first_name",
                "last_name",
                "email",
                "spendings"
              ]
            }
          },
          "required": [
            "id",
            "type",
            "attributes"
          ]
        }
      },
      "required": [
        "data"
      ]
    },
    "jwt": {
      "type": "string"
    }
  },
  "required": [
    "user",
    "jwt"
  ]
}
